<?xml version="1.0" encoding="utf-8"?>
<shape
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <corners
        android:radius="30dp"
        />

    <size
        android:width="150dp"
        android:height="40dp" />

    <solid
        android:id="@+id/thumbColor"
        android:color="#EEEEEE" />

    <stroke
        android:id="@+id/strokeColor"
        android:width="1dp"
        android:color="#EEEEEE" />

</shape>